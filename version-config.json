{
  "immortalwrt_version": "24.10",
  "device": "onecloud",
  "architecture": "armv7",
  "default_ip": "192.168.2.2",
  "username": "root",
  "password": "",
  "features": {
    "luci": true,
    "nikki": true,
    "ttyd": true,
    "docker": true,
    "auto_expand_storage": true,
    "monthly_updates": true
  },
  "packages": [
    "luci",
    "luci-ssl-openssl",
    "luci-app-dockerman",
    "nikki",
    "ttyd",
    "docker",
    "dockerd",
    "docker-compose"
  ],
  "storage": {
    "rootfs_size": 7000,
    "auto_expand": true,
    "docker_path": "/opt/docker",
    "software_path": "/opt/software"
  }
}
